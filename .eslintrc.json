{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "jest"
    ],
    "env": {
        "jest/globals": true,
        "browser": true
    },
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "next"
            }
        ],
        "react/prop-types": [
            0
        ]
    },
    "import/no-extraneous-dependencies": [
        "error",
        {
            "devDependencies": true
        }
    ]
}
